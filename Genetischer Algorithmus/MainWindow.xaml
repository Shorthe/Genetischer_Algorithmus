<Window x:Class="Genetic_Algorithm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="512" Width="768">
    <Grid>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="68,160,0,0" Name="tbUpperBound" VerticalAlignment="Top" Width="39" Text="100" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="23,160,0,0" Name="tbLowerBound" VerticalAlignment="Top" Width="39" Text="-100" />
        <Label Content="Grenzen" Height="28" HorizontalAlignment="Left" Margin="23,130,0,0" Name="label1" VerticalAlignment="Top" Width="84" />
        <Label Content="Speichermodus" Height="28" HorizontalAlignment="Left" Margin="473,17,0,0" Name="label2" VerticalAlignment="Top" Width="120" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="473,51,0,0" Name="cbGenType" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="boolean" IsSelected="True" />
            <ComboBoxItem Content="decimal" />
        </ComboBox>
        <Button Content="Algorithmus ausführen" Height="23" HorizontalAlignment="Left" Margin="164,160,0,0" Name="button1" VerticalAlignment="Top" Width="153" Click="button1_Click" />
        <TextBlock Height="256" HorizontalAlignment="Left" Margin="23,205,0,0" Name="textBlock1" Text="TextBlock" VerticalAlignment="Top" Width="412" />
        <Canvas Height="256" HorizontalAlignment="Left" Margin="441,205,0,0" Name="canvas1" VerticalAlignment="Top" Width="293" />
        <CheckBox Content="variable Mutationsrate" Height="16" HorizontalAlignment="Left" Margin="197,17,0,0" Name="checkBox1" VerticalAlignment="Top" />
        <Slider Height="23" HorizontalAlignment="Left" Margin="197,46,0,0" Name="slider1" VerticalAlignment="Top" Width="136" Minimum="1" IsMoveToPointEnabled="False" IsSnapToTickEnabled="True" TickPlacement="BottomRight" UseLayoutRounding="False" SelectionEnd="5" SmallChange="1" Maximum="10" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="124,72,0,0" Name="tbRekombinationen" VerticalAlignment="Top" Width="39" Text="100" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="124,101,0,0" Name="tbMutationen" VerticalAlignment="Top" Width="39" Text="30" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="124,43,0,0" Name="tbGenerationen" VerticalAlignment="Top" Width="39" FlowDirection="LeftToRight" Text="1000" />
        <Label Content="Generationen" Height="28" HorizontalAlignment="Left" Margin="12,41,0,0" Name="label3" VerticalAlignment="Top" Width="106" />
        <Label Content="Rekombinationen" Height="28" HorizontalAlignment="Left" Margin="12,70,0,0" Name="label4" VerticalAlignment="Top" Width="106" />
        <Label Content="Mutationen" Height="28" HorizontalAlignment="Left" Margin="12,99,0,0" Name="label5" VerticalAlignment="Top" Width="106" />
        <Label Content="Gleichungssystem" Height="28" HorizontalAlignment="Left" Margin="473,101,0,0" Name="label6" VerticalAlignment="Top" Width="120" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="473,135,0,0" Name="cbSystemOfEquation" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="Standard" IsSelected="True" />
            <ComboBoxItem Content="Benchmark" />
        </ComboBox>
    </Grid>
</Window>
